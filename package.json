{
  "name": "react-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelabomfim/react-starter.git"
  },
  "author": "Marcela Bomfim <mbomfim@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelabomfim/react-starter/issues"
  },
  "homepage": "https://marcelabomfim.github.io/react-starter",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "gh-pages": "^1.2.0",
    "polished": "^1.9.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-scripts": "1.1.4",
    "react-snapshot": "^1.3.0",
    "styled-components": "^3.3.0",
    "styled-normalize": "^4.0.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4"
  }
}
